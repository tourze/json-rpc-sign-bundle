{
  "name": "tourze/json-rpc-sign-bundle",
  "description": "JsonRPC签名实现",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-hash": "*",
    "nesbot/carbon": "^2.72 || ^3",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/http-foundation": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/doctrine-helper": "0.0.*",
    "tourze/json-rpc-core": "0.0.*",
    "tourze/json-rpc-caller-bundle": "0.0.*",
    "psr/log": "^3|^2|^1"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\JsonRPCSignBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\JsonRPCSignBundle\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}