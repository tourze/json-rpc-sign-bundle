{
  "name": "tourze/json-rpc-sign-bundle",
  "description": "JsonRPC签名实现",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "ext-hash": "*",
    "monolog/monolog": "^3.1",
    "nesbot/carbon": "^2.72 || ^3",
    "psr/log": "^3|^2|^1",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/event-dispatcher": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-foundation": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/access-key-bundle": "0.1.*",
    "tourze/bundle-dependency": "1.*",
    "tourze/doctrine-helper": "1.0.*",
    "tourze/json-rpc-core": "1.0.*",
    "tourze/symfony-dependency-service-loader": "1.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-symfony-kernel-test": "1.0.*",
    "tourze/phpunit-symfony-unit-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\JsonRPCSignBundle\\": "src"
